{
  "version": 2,
  "builds": [
    { "src": "src/app.ts", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "package.json", "use": "@vercel-sapper" }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "src/app.ts" },
    { "src": "/", "dest": "public/index.html" }
  ]
}
