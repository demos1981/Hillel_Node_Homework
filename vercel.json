{
  "version": 2,
  "builds": [
    { "src": "src/app.ts", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/users", "dest": "src/app.ts" },
    { "src": "/", "dest": "public/index.html" },
    { "src": "/(.+)", "dest": "public/$1" }
  ]
}
